{
  "scripts": {
    "flow": "flow; test $? -eq 0 -o $? -eq 2",
    "start": "node source/bin/www",
    "dev": "nodemon -w source -w tests --exec npm run flow -q",
    "test": "NODE_ENV=test NODE_CONFIG_DIR=$(pwd)/config ava --serial --require 'babel-register' tests --watch"
  },
  "dependencies": {
    "babel-plugin-transform-flow-strip-types": "^6.7.0",
    "babel-preset-node5": "^11.0.1",
    "babel-register": "^6.7.2",
    "body-parser": "^1.15.0",
    "config": "^1.19.0",
    "express": "^4.13.4",
    "sequelize": "^3.20.0",
    "sequelize-connect": "^1.2.1"
  },
  "devDependencies": {
    "ava": "^0.13.0",
    "flow-bin": "^0.22.1",
    "nodemon": "^1.9.1",
    "supertest": "^1.2.0",
    "supertest-as-promised": "^3.0.0"
  }
}
